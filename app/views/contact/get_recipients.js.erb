<% if @sens.nil? && @reps.nil? %>
  $j('#contact_recipients_container').html("<div class='none'>We could not find your representatives with the info you provided.  Please try again.");
<% else %>
  $j('#contact_recipients_container').html("<%= escape_javascript(render :partial => 'contact/contact_recipients', :locals => { :show_checkboxes => true }) %>");
  $j('#contribution_data_container').html("<%= escape_javascript(render :partial => 'contact/message_builder_contribution_data') %>");
<% end %>